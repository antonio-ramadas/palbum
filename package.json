{
    "name": "palbum",
    "version": "0.1.0",
    "author": "AntÃ³nio Ramadas",
    "license": "MIT",
    "keywords": [
        "spotify"
    ],
    "homepage": "./",
    "repository": "github:antonio-ramadas/palbum",
    "description": "Resume playing Spotify albums/playlists (no long start from the beginning)",
    "scripts": {
        "start": "npm run start-dev",
        "start-dev": "concurrently \"BROWSER=none npm run react\" \"wait-on http://localhost:3000 && npm run electron\"",
        "react": "react-scripts start",
        "electron": "electron .",
        "test": "react-scripts test",
        "lint": "node_modules/.bin/eslint .",
        "postinstall": "chmod +x postinstall.sh && sh postinstall.sh",
        "build": "react-scripts build",
        "eject": "react-scripts eject"
    },
    "dependencies": {
        "electron": "^3.0.10",
        "electron-is-dev": "^1.0.1",
        "electron-store": "^2.0.0",
        "fuse.js": "^3.3.0",
        "menubar": "^5.2.3",
        "react": "^16.6.3",
        "react-dom": "^16.6.3",
        "react-scripts": "^2.1.3",
        "spotify-authentication": "0.3.0",
        "spotify-context-history": "^0.3.1",
        "spotify-web-api-node": "^4.0.0"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "concurrently": "^4.1.0",
        "eslint-config-airbnb": "^17.1.0",
        "wait-on": "^3.2.0"
    }
}
